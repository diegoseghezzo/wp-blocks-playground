{
	"name": "diegoseghezzo/times-news-block",
	"description": "WordPress Gutenberg block for displaying The Times news with AI filtering",
	"type": "wordpress-plugin",
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Diego Seghezzo",
			"email": "diego@example.com"
		}
	],
	"require": {
		"php": ">=7.4"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.0",
		"yoast/phpunit-polyfills": "^1.0"
	},
	"autoload": {
		"classmap": [
			"includes/"
		]
	},
	"scripts": {
		"test": "phpunit",
		"test:coverage": "phpunit --coverage-html coverage"
	}
}
